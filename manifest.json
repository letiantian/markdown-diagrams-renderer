{
    "manifest_version": 2,
    "name": "markdown diagrams render",
    "version": "0.1",
    "web_accessible_resources": ["inject/*.js", "inject/*.css"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": ["activeTab"],

    "icons": { "48": "icon.png" }
}